  "dependencies": {
    "express": "5.2.1",
    "puppeteer": "24.33.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "25.0.3",
    "eslint": "9.39.2",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0"
  }
